{
    "label" : "wb-t-on",
    "action" : "exec -a shellTouchbar waybar -c /neoservice/Scripts/wm/waybar-touch/config -s /neoservice/Scripts/wm/waybar-touch/style.css",
    "text" : "Touchbar ON",
    "keybind" : "+"
}
{
    "label" : "wb-t-off",
    "action" : "pkill -f shellTouchbar",
    "text" : "Touchbar OFF",
    "keybind" : "-"
